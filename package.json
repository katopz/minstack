{
  "name": "minstack",
  "version": "0.1.0",
  "description": "Minimal and Fast Stack Starter Kit for Web Applications.",
  "scripts": {
    "dev": "npm run env NODE_ENV=development&&webpack-dev-server --inline --hot --progress",
    "start": "superstatic build -p ${PORT:-8080} --host 0.0.0.0 --gzip -c '{\"rewrites\": [{\"source\":\"**\",\"destination\":\"index.html\"}],\"headers\":[{\"source\":\"**\",\"headers\":[{\"key\":\"Cache-Control\",\"value\":\"max-age=86400\"}]}]}'",
    "prestart": "npm run build",
    "prebuild": "mkdirp build",
    "build": "npm run env NODE_ENV=production&&ncp src/index.html build/index.html webpack -p",
    "deploy": "gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/katopz/minstack.git"
  },
  "keywords": [
    "preact",
    "reux",
    "pouchdb"
  ],
  "author": "katopz <katopz@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-class-properties": "^6.5.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "gh-pages": "^0.10.0",
    "http-server": "^0.9.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "pouchdb-server": "^1.0.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "decko": "^1.1.3",
    "preact": "^4.0.1",
    "preact-router": "^1.2.4",
    "todomvc-app-css": "^2.0.3",
    "todomvc-common": "^1.0.2",
    "pouchdb": "^5.3.0"
  }
}
